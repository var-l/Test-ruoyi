<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.ruoyi.system.mapper.SysDeviceMapper">    <resultMap id="SysDevice" type="SysDevice">        <id property="id" column="id"/>        <result property="deviceSn" column="device_Sn"/>        <result property="deviceName" column="device_name"/>        <result property="location" column="location"/>        <result property="createTime" column="create_at"/>        <result property="status" column="status"/>        <result property="updateTime" column="updated_at"/>        <result property="remark" column="remark"/>        <result property="deleteAt" column="delete_at"/>    </resultMap>    <!--id, device_Sn, device_name, location, status, create_at, updated_at, remake,delete_at-->    <select id="selectDeviceList" parameterType="SysDevice" resultMap="SysDevice">        SELECT *        FROM device        WHERE delete_at IS NULL        <if test="deviceName != null and deviceName != ''">            AND device_name LIKE CONCAT('%', #{deviceName}, '%')        </if>    </select>    <insert id="insert" parameterType="SysDevice" useGeneratedKeys="true" keyProperty="id">        insert into device (        <if test="id != null and id != 0">id,</if>        <if test="deviceName != null and deviceName != ''">device_name,</if>        <if test="deviceSn != null and deviceSn != ''">device_Sn,</if>        <if test="location != null and location != ''">location,</if>        <if test="status != null and status != ''">status,</if>        <if test="remark != null and remark != ''">remark,</if>        create_at        ) values (        <if test="id != null and id != 0">#{id},</if>        <if test="deviceName != null and deviceName != ''">#{deviceName},</if>        <if test="deviceSn != null and deviceSn != ''">#{deviceSn},</if>        <if test="location != null and location != ''">#{location},</if>        <if test="status != null and status != ''">#{status},</if>        <if test="remark != null and remark != ''">#{remark},</if>        sysdate()        )    </insert></mapper>